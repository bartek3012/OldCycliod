<Window x:Class="OldCycliod.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OldCycliod"
        mc:Ignorable="d"
        Title="MainWindow" Height="550" Width="770" ResizeMode="CanMinimize"
        WindowStartupLocation="Manual" Left="300" Top="100">
    <Window.Background>
        <ImageBrush ImageSource="/Background.jpg" />
    </Window.Background>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="10*"/>
            <ColumnDefinition Width="7*"/>
        </Grid.ColumnDefinitions>
        <Image Grid.Column="1" Margin="-30,-80,-31,-36"  Source="/sprze.png" />
        <Frame VerticalAlignment="Top" x:Name="InputOutputPages" Grid.Column="0" Height="470" Width="420"/>
    </Grid>
</Window>
